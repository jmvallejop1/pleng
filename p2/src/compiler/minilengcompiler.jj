{
  " "
| "\r"
| "\t"
| "\n"
| "%" : COMENTARIO
| "%%": COMENTARIO_MULTILINEA
}

< COMENTARIO_MULTILINEA> SKIP:
{
  "%%": DEFAULT
  | < ~[] >  
}


< COMENTARIO >SKIP: {
	"\n":DEFAULT
	| < ~[] >  
}



